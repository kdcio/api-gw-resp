{
  "name": "@kdcsoftware/api-gw-resp",
  "version": "1.0.1",
  "description": "API Gateway response builder",
  "main": "lib/index.js",
  "typings": "lib/index.d.ts",
  "files": [
    "lib/*"
  ],
  "type": "module",
  "scripts": {
    "build": "npm run clean && npm run bundle",
    "build:commonjs": "babel src --out-dir lib",
    "clean": "rimraf lib dist",
    "test": "jest --no-cache",
    "watch": "jest --watchAll",
    "coverage": "npm test -- --coverage",
    "lint:fix": "eslint src --ext .js --fix",
    "lint": "eslint src --ext .ts",
    "bundle": "ncc build src/index.ts -m -o lib",
    "tsc": "tsc src/index.ts --noEmit"
  },
  "author": "Ian Dela Cruz <ian@kdcsoftware.com> (https://www.kdcsoftware.com)",
  "repository": {
    "type": "git",
    "url": "https://github.com/kdcsoftware/api-gw-resp"
  },
  "homepage": "https://github.com/kdcsoftware/api-gw-resp",
  "license": "MIT",
  "keywords": [
    "kdc",
    "aws",
    "api",
    "gateway",
    "lambda",
    "response"
  ],
  "dependencies": {
    "@babel/runtime": "^7.9.6"
  },
  "devDependencies": {
    "@babel/cli": "^7.8.4",
    "@babel/core": "^7.9.6",
    "@babel/plugin-transform-runtime": "^7.9.6",
    "@babel/preset-env": "^7.9.6",
    "@types/jest": "^25.2.3",
    "@typescript-eslint/eslint-plugin": "^3.0.0",
    "@typescript-eslint/parser": "^3.0.0",
    "@zeit/ncc": "^0.22.3",
    "babel-eslint": "^10.1.0",
    "babel-jest": "^26.0.1",
    "eslint": "^6.8.0",
    "eslint-config-airbnb-base": "^14.1.0",
    "eslint-config-airbnb-typescript": "^7.2.1",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-import": "^2.20.2",
    "eslint-plugin-jest": "^23.13.1",
    "eslint-plugin-prettier": "^3.1.3",
    "husky": "^4.2.5",
    "jest": "^26.0.1",
    "lint-staged": "^10.2.2",
    "prettier": "^2.0.5",
    "pretty-quick": "^2.0.1",
    "regenerator-runtime": "^0.13.5",
    "rimraf": "^3.0.2",
    "ts-jest": "^26.0.0",
    "typescript": "^3.9.3"
  },
  "targets": {
    "main": {
      "engines": {
        "node": ">=10.x"
      }
    }
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.js": [
      "eslint --fix",
      "pretty-quick --staged",
      "git add"
    ]
  }
}
